<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.phalurao.kotlinedemo">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:name=".App"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Webloading"/>
        <activity android:name=".listjson.ListAct"/>
        <activity android:name=".volleyrequestpack.VolleyNewAct"/>
        <activity android:name=".ui_components.UiComponentAct"/>
        <activity android:name=".customrecycle.CustomRecycleAct"/>
        <activity android:name=".plursight.DemoMainActivity"/>
        <activity android:name=".plursight.DisplayActivity"/>

    </application>

</manifest>